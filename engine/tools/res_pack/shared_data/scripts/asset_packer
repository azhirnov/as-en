	static constexpr uint YNeg = 3;
	static constexpr uint ZPos = 4;
	static constexpr uint ZNeg = 5;
};

struct Texture
{
	Texture ();
	void  Alloc (const uint2 &, EPixelFormat);
	void  Alloc (const uint2 &, EPixelFormat, const ImageLayer &);
	void  Alloc (const uint2 &, EPixelFormat, const MipmapLevel &);
	void  Alloc (const uint2 &, EPixelFormat, const ImageLayer &, const MipmapLevel &);
	void  Alloc (const uint3 &, EPixelFormat);
	void  Alloc (const uint3 &, EPixelFormat, const MipmapLevel &);
	void  AllocCube (const uint2 &, EPixelFormat);
	void  AllocCube (const uint2 &, EPixelFormat, const MipmapLevel &);
	void  Load (const string &);
	void  Load (const string &, bool);
	void  Load (const string &, const RectU &);
	void  AddLayer (const string &, uint);
	void  AddLayer (const string &, uint, bool);
	void  AddLayer (const string &, const RectU &, uint);
	void  LoadChannel (const string &, const string &, const string &);
	void  Store (const string &);
	void  Format (EPixelFormat);
};

struct ImageAtlas
{
	ImageAtlas ();
	void  Add (const string &, const string &);
	void  Add (const string &, const string &, const RectU &);
	void  Store (const string &);
	void  Padding (uint);
	void  Format (EPixelFormat);
};

struct ERasterFontMode
{
	ERasterFontMode () {}
	ERasterFontMode (uint) {}
	operator uint () const;
	static constexpr uint Raster = 0;
	static constexpr uint SDF = 1;
	static constexpr uint MC_SDF = 2;
};

struct RasterFont
{
	RasterFont ();
	void  Load (const string &);
	void  AddCharset (uint, uint);
	void  AddCharset_Ascii ();
	void  AddCharset_Rus ();
	void  ClearCharset ();
	void  GlyphSize (uint);
	void  Padding (uint);
	void  Store (const string &);
	void  Format (EPixelFormat);
	void  RasterMode (ERasterFontMode);
	void  SDFPixelRange (uint);
};

struct Mesh
{
	Mesh ();
};

struct Model
{
	Model ();
};

struct Material
{
	Material ();
};

struct UIColorStyle
{
	void  Disabled (const RGBA8u &);
	void  Enabled (const RGBA8u &);
	void  MouseOver (const RGBA8u &);
	void  TouchDown (const RGBA8u &);
	void  Selected (const RGBA8u &);
	void  Pipeline (const string &);
};

struct UIImageStyle
{
	void  Disabled (const RGBA8u &, const string &);
	void  Enabled (const RGBA8u &, const string &);
	void  MouseOver (const RGBA8u &, const string &);
	void  TouchDown (const RGBA8u &, const string &);
	void  Selected (const RGBA8u &, const string &);
	void  Pipeline (const string &);
};

struct UIFontStyle
{
	void  Disabled (const RGBA8u &);
	void  Enabled (const RGBA8u &);
	void  MouseOver (const RGBA8u &);
	void  TouchDown (const RGBA8u &);
	void  Selected (const RGBA8u &);
	void  Font (const string &);
	void  Pipeline (const string &);
};

struct UIStyleCollection
{
	UIStyleCollection ();
	void  Atlas (const string &);
	RC<UIColorStyle>  AddColorStyle (const string &);
	RC<UIImageStyle>  AddImageStyle (const string &);
	RC<UIFontStyle>  AddFontStyle (const string &);
	void  Store (const string &);
};

template <>
struct RC<UIColorStyle> : UIColorStyle
{
	RC (const UIColorStyle &);
};

template <>
struct RC<Texture> : Texture
{
	RC (const Texture &);
};

template <>
struct RC<Material> : Material
{
	RC (const Material &);
};

template <>
struct RC<Mesh> : Mesh
{
	RC (const Mesh &);
};

template <>
struct RC<ImageAtlas> : ImageAtlas
{
	RC (const ImageAtlas &);
};

template <>
struct RC<UIImageStyle> : UIImageStyle
{
	RC (const UIImageStyle &);
};

template <>
struct RC<RasterFont> : RasterFont
{
	RC (const RasterFont &);
};

template <>
struct RC<Model> : Model
{
	RC (const Model &);
};

template <>
struct RC<UIStyleCollection> : UIStyleCollection
{
	RC (const UIStyleCollection &);
};

template <>
struct RC<UIFontStyle> : UIFontStyle
{
	RC (const UIFontStyle &);
};

